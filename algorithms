#include <iostream>
using namespace std;
int main() {
    int result[15];
    for (int k = 0; k < 15; k++) {
        int operation_counter = 0;
        int n;
        cin >> n;
        int** matrix = new int* [n];                //создание матрицы и её заполнение
        for (int i = 0; i < n; i++) {
            matrix[i] = new int[n];
        }

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                matrix[i][j] = 1;
                operation_counter++;
            }
        }

        int* vector = new int[n];                  //создание вектора и его заполнение
        for (int i = 0; i < n; i++) {
            vector[i] = 2;
            operation_counter++;
        }

        for (int i = 0; i < n; i++) {               //перемножаем элементы вектора на элементы матрицы и получаем новые значения в матрице
            for (int j = 0; j < n; j++) {
                matrix[i][j] *= vector[i];
                operation_counter += 2;
            }
        }

        for (int i = 0; i < n; i++) {
            vector[i] = 0;                          //обнуляем элементы вектора для записи новых ответов
        }

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                vector[i] += matrix[i][j];         //записываем в вектор сумму значений строки, которые получились в матрице
                operation_counter += 2;
            }
        }

        result[k] = operation_counter;
    }

    cout << endl;
    for (int i = 0; i < 15; i++) {
        cout << result[i] << " ";
    }
    cout << endl;
}
